pipline{

    agent none
    stages{
        stage('build'){
            steps{
                agent {
                    docker {image 'maven:3.8.1-adoptopenjdk-11' }
                }
                sh 'mvn install'
            }
        }
        stage('frount end'){
            agent{
                docker {image 'node:16-alpine'}
            }
            sh 'npm install'
        }
    }

}
